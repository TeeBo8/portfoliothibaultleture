[32m+[m[32mconst fs = require('fs');[m
[32m+[m[32mconst path = require('path');[m
[32m+[m
[32m+[m[32m// V√©rifier si un r√©pertoire existe[m
[32m+[m[32mfunction directoryExists(dirPath) {[m
[32m+[m[32m  try {[m
[32m+[m[32m    return fs.statSync(dirPath).isDirectory();[m
[32m+[m[32m  } catch (err) {[m
[32m+[m[32m    return false;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Fonction pour parcourir r√©cursivement un r√©pertoire[m
[32m+[m[32mfunction walkDir(dir, callback) {[m
[32m+[m[32m  console.log(`Exploration du r√©pertoire: ${dir}`);[m
[32m+[m[41m  [m
[32m+[m[32m  if (!directoryExists(dir)) {[m
[32m+[m[32m    console.error(`Le r√©pertoire ${dir} n'existe pas!`);[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m[41m  [m
[32m+[m[32m  const files = fs.readdirSync(dir);[m
[32m+[m[32m  console.log(`Nombre de fichiers/dossiers trouv√©s: ${files.length}`);[m
[32m+[m[41m  [m
[32m+[m[32m  files.forEach(f => {[m
[32m+[m[32m    let fullPath = path.join(dir, f);[m
[32m+[m[32m    console.log(`Examen de: ${fullPath}`);[m
[32m+[m[41m    [m
[32m+[m[32m    let isDirectory = fs.statSync(fullPath).isDirectory();[m
[32m+[m[32m    if (isDirectory) {[m
[32m+[m[32m      console.log(`${fullPath} est un r√©pertoire, exploration r√©cursive...`);[m
[32m+[m[32m      walkDir(fullPath, callback);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      console.log(`${fullPath} est un fichier, traitement...`);[m
[32m+[m[32m      callback(fullPath);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Fonction pour corriger les fins de ligne dans un fichier MDX[m
[32m+[m[32mfunction fixMdxFile(filePath) {[m
[32m+[m[32m  if (!filePath.endsWith('.mdx')) {[m
[32m+[m[32m    console.log(`Ignor√©: ${filePath} (pas un fichier MDX)`);[m
[32m+[m[32m    return;[m
[32m+[m[32m  }[m
[32m+[m[41m  [m
[32m+[m[32m  console.log(`Traitement du fichier MDX: ${filePath}`);[m
[32m+[m[41m  [m
[32m+[m[32m  try {[m
[32m+[m[32m    // Lire le contenu du fichier[m
[32m+[m[32m    let content = fs.readFileSync(filePath, 'utf8');[m
[32m+[m[32m    console.log(`Contenu lu: ${content.length} caract√®res`);[m
[32m+[m[41m    [m
[32m+[m[32m    // Afficher les premiers caract√®res pour d√©bogage[m
[32m+[m[32m    console.log(`D√©but du contenu: ${content.substring(0, 100).replace(/\r/g, '\\r').replace(/\n/g, '\\n')}...`);[m
[32m+[m[41m    [m
[32m+[m[32m    // V√©rifier si le fichier contient des fins de ligne Windows[m
[32m+[m[32m    const hasWindowsLineEndings = content.includes('\r\n');[m
[32m+[m[32m    const hasPublishedTrueR = content.includes('published: true\r');[m
[32m+[m[41m    [m
[32m+[m[32m    console.log(`Fins de ligne Windows: ${hasWindowsLineEndings}`);[m
[32m+[m[32m    console.log(`Probl√®me "published: true\\r": ${hasPublishedTrueR}`);[m
[32m+[m[41m    [m
[32m+[m[32m    if (hasWindowsLineEndings || hasPublishedTrueR) {[m
[32m+[m[32m      console.log(`Fichier n√©cessitant des corrections: ${filePath}`);[m
[32m+[m[41m      [m
[32m+[m[32m      // Remplacer les fins de ligne Windows par des fins de ligne Unix[m
[32m+[m[32m      if (hasWindowsLineEndings) {[m
[32m+[m[32m        content = content.replace(/\r\n/g, '\n');[m
[32m+[m[32m        console.log(`Fins de ligne Windows corrig√©es`);[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      // Corriger sp√©cifiquement le probl√®me "published: true\r"[m
[32m+[m[32m      if (hasPublishedTrueR) {[m
[32m+[m[32m        content = content.replace(/published: true\r/g, 'published: true');[m
[32m+[m[32m        console.log(`Probl√®me "published: true\\r" corrig√©`);[m
[32m+[m[32m      }[m
[32m+[m[41m      [m
[32m+[m[32m      // √âcrire le contenu corrig√© dans le fichier[m
[32m+[m[32m      fs.writeFileSync(filePath, content, 'utf8');[m
[32m+[m[41m      [m
[32m+[m[32m      console.log(`Fichier sauvegard√© avec succ√®s`);[m
[32m+[m[32m    } else {[m
[32m+[m[32m      console.log(`Aucune correction n√©cessaire pour ${filePath}`);[m
[32m+[m[32m    }[m
[32m+[m[32m  } catch (error) {[m
[32m+[m[32m    console.error(`Erreur lors du traitement du fichier ${filePath}:`, error);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Parcourir le r√©pertoire content et corriger tous les fichiers MDX[m
[32m+[m[32mconsole.log('D√©but de la correction des fichiers MDX...');[m
[32m+[m
[32m+[m[32mconst contentDir = './content';[m
[32m+[m[32mif (directoryExists(contentDir)) {[m
[32m+[m[32m  console.log(`Le r√©pertoire ${contentDir} existe, d√©but du traitement...`);[m
[32m+[m[32m  walkDir(contentDir, fixMdxFile);[m
[32m+[m[32m} else {[m
[32m+[m[32m  console.error(`ERREUR: Le r√©pertoire ${contentDir} n'existe pas!`);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconsole.log('Correction des fichiers MDX termin√©e.');[m[41m [m
